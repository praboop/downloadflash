{
  "description": "Downloads Files in the current Web page",
  "manifest_version": 2,
  "name": "Download Links",
  "version": "2.2",
  "icons": {
    "48": "icons/RedButton48.ico",
    "96": "icons/RedButton96.ico"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/RedButton96.ico",
    "default_title": "Download Files in this Web page",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "downloads",
    "downloads.open"
  ]
}